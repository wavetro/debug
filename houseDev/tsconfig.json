{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "DOM",
      "ES6"
    ],
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noResolve": false,
    "removeComments": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}